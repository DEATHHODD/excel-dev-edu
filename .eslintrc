{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens":"off",
        "linebreak-style":"off",
        "indent":"off",
        "comma-dangle":"off",
        "space-before-blocks":"off",
        "require-jsdoc":"off"
    },
    "extends": ["eslint:recommended", "google"]
}